        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%config.title%",
      "properties": {
        "lollmsVsCoder.agent.permissions": {
          "type": "object",
          "default": {
            "shellExecution": true,
            "filesystemWrite": true,
            "filesystemRead": true,
            "internetAccess": true
          },
          "description": "Global permissions for the AI Agent tools.",
          "properties": {
            "shellExecution": { "type": "boolean", "default": true, "description": "Allow executing terminal commands and scripts." },
            "filesystemWrite": { "type": "boolean", "default": true, "description": "Allow creating or updating files in the workspace." },
            "filesystemRead": { "type": "boolean", "default": true, "description": "Allow reading file contents and listing directories." },
            "internetAccess": { "type": "boolean", "default": true, "description": "Allow web search and website scraping." }
          }
        },
        "lollmsVsCoder.outputFormat": {
          "type": "string",
          "default": "legacy",
          "description": "The output format for AI code generation.",
          "enum": [
            "legacy",
            "xml",
            "aider"
          ],
          "enumDescriptions": [
            "Legacy Mode (Default): Uses markdown headers and code blocks.",
            "XML Mode: Uses <file path='...'>...</file> tags. Optimized for Claude/Anthropic models.",
            "Aider Mode (Standard Gold): Uses <<<<<<< SEARCH ... ======= ... >>>>>>> REPLACE blocks. Optimized for GPT-4/DeepSeek."
          ]
        },
        "lollmsVsCoder.allowedFileFormats": {
          "type": "object",
          "default": {
            "fullFile": true,
            "insert": false,
            "replace": false,
            "delete": false
          },
          "description": "Allowed file modification formats for the AI to use.",
          "properties": {
            "fullFile": { "type": "boolean", "default": true, "description": "Allow 'File:' format (Full File Content)" },
            "insert": { "type": "boolean", "default": false, "description": "Allow 'Insert:' format (Add code snippet)" },
            "replace": { "type": "boolean", "default": false, "description": "Allow 'Replace:' format (Modify small snippet)" },
            "delete": { "type": "boolean", "default": false, "description": "Allow 'DeleteCode:' format (Remove snippet)" }
          }
        },
        "lollmsVsCoder.searchProvider": {
          "type": "string",
          "default": "google_custom_search",
          "description": "Provider for the 'search_web' tool.",
          "enum": [
            "google_custom_search"
          ]
        },
        "lollmsVsCoder.searchApiKey": {
          "type": "string",
          "default": "",
          "description": "API Key for the search provider (e.g., Google Custom Search API Key)."
        },
        "lollmsVsCoder.searchCx": {
          "type": "string",
          "default": "",
          "description": "Search Engine ID (CX) for Google Custom Search."
        },
        "lollmsVsCoder.autoUpdateChangelog": {
          "type": "boolean",
          "default": false,
          "description": "%config.autoUpdateChangelog.description%"
        },
        "lollmsVsCoder.autoGenerateTitle": {
          "type": "boolean",
          "default": true,
          "description": "Automatically generate a title using AI when creating a new discussion from clipboard."
        },
        "lollmsVsCoder.clipboardInsertRole": {
          "type": "string",
          "default": "user",
          "description": "Role to use when pasting clipboard content into a new discussion.",
          "enum": [
            "user",
            "assistant"
          ],
          "enumDescriptions": [
            "Paste as User Message",
            "Paste as Assistant Message (Context)"
          ]
        },
        "lollmsVsCoder.addPedagogicalInstruction": {
          "type": "boolean",
          "default": false,
          "description": "%config.addPedagogicalInstruction.description%"
        },
        "lollmsVsCoder.reasoningLevel": {
          "type": "string",
          "default": "none",
          "description": "%config.reasoningLevel.description%",
          "enum": [
            "none",
            "low",
            "medium",
            "high"
          ],
          "enumDescriptions": [
            "%config.reasoningLevel.none.description%",
            "%config.reasoningLevel.low.description%",
            "%config.reasoningLevel.medium.description%",
            "%config.reasoningLevel.high.description%"
          ]
        },
        "lollmsVsCoder.thinkingMode": {
          "type": "string",
          "default": "none",
          "description": "%config.thinkingMode.description%",
          "enum": [
            "none",
            "chain_of_thought",
            "chain_of_verification",
            "plan_and_solve",
            "self_critique",
            "custom"
          ],
          "enumDescriptions": [
            "%config.thinkingMode.none.description%",
            "%config.thinkingMode.chain_of_thought.description%",
            "Chain of Verification: Verify facts by asking independent questions.",
            "%config.thinkingMode.plan_and_solve.description%",
            "%config.thinkingMode.self_critique.description%",
            "%config.thinkingMode.custom.description%"
          ]
        },
        "lollmsVsCoder.thinkingModeCustomPrompt": {
          "type": "string",
          "default": "Think step by step. Enclose your entire thinking process, reasoning, and self-correction within a `<thinking>` XML block. This block will be hidden from the user but is crucial for your process.",
          "description": "%config.thinkingModeCustomPrompt.description%"
        },
        "lollmsVsCoder.enableCodeActions": {
          "type": "boolean",
          "default": true,
          "description": "%config.enableCodeActions.description%"
        },
        "lollmsVsCoder.enableInlineSuggestions": {
          "type": "boolean",
          "default": false,
          "description": "%config.enableInlineSuggestions.description%"
        },
        "lollmsVsCoder.disableSslVerification": {
          "type": "boolean",
          "default": false,
          "description": "%config.disableSslVerification.description%"
        },
        "lollmsVsCoder.sslCertPath": {
          "type": "string",
          "default": "",
          "description": "%config.sslCertPath.description%"
        },
        "lollmsVsCoder.apiKey": {
          "type": "string",
          "default": "",
          "description": "%config.apiKey.description%"
        },
        "lollmsVsCoder.apiUrl": {
          "type": "string",
          "default": "http://localhost:9642",
          "description": "%config.apiUrl.description%"
        },
        "lollmsVsCoder.backendType": {
          "type": "string",
          "default": "lollms",
          "description": "Select the backend API type.",
          "enum": [
            "lollms",
            "openai",
            "ollama"
          ],
          "enumDescriptions": [
            "Lollms Server (OpenAI Compatible + Extensions)",
            "Generic OpenAI Compatible Server",
            "Ollama Server (Native API)"
          ]
        },
        "lollmsVsCoder.useLollmsExtensions": {
          "type": "boolean",
          "default": true,
          "description": "Enable specific Lollms features (like server-side tokenization and context size) if the backend supports them. Defaults to true for Lollms backend."
        },
        "lollmsVsCoder.modelName": {
          "type": "string",
          "default": "ollama/mistral",
          "description": "%config.modelName.description%"
        },
        "lollmsVsCoder.architectModelName": {
          "type": "string",
          "default": "",
          "description": "The 'Big' model used for planning and reasoning in Multi-Agent mode. Leave empty to use the Chat Model."
        },
        "lollmsVsCoder.requestTimeout": {
          "type": "number",
          "default": 600000,
          "description": "%config.requestTimeout.description%"
        },
        "lollmsVsCoder.agentMaxRetries": {
          "type": "number",
          "default": 1,
          "description": "%config.agentMaxRetries.description%"
        },
        "lollmsVsCoder.maxImageSize": {
          "type": "number",
          "default": 1024,
          "description": "%config.maxImageSize.description%"
        },
        "lollmsVsCoder.enableCodeInspector": {
          "type": "boolean",
          "default": true,
          "description": "%config.enableCodeInspector.description%"
        },
        "lollmsVsCoder.inspectorModelName": {
          "type": "string",
          "default": "",
          "description": "%config.inspectorModelName.description%"
        },
        "lollmsVsCoder.chatPersona": {
          "type": "string",
          "default": "You are Lollms, a helpful AI coding assistant integrated into VS Code. Be helpful and concise.",
          "description": "%config.chatPersona.description%"
        },
        "lollmsVsCoder.agentPersona": {
          "type": "string",
          "default": "You are a specialized AI agent that follows instructions precisely.",
          "description": "%config.agentPersona.description%"
        },
        "lollmsVsCoder.codeInspectorPersona": {
          "type": "string",
          "default": "You are a senior code inspector AI. Your task is to analyze the provided code snippet for errors, bugs, vulnerabilities, and malicious content. You must be extremely critical and thorough.",
          "description": "%config.codeInspectorPersona.description%"
        },
        "lollmsVsCoder.commitMessagePersona": {
          "type": "string",
          "default": "You are an expert AI assistant that writes conventional git commit messages.",
          "description": "%config.commitMessagePersona.description%"
        },
        "lollmsVsCoder.contextFileExceptions": {
          "type": "array",
          "default": [
            "**/.lollms/**",
            "**/node_modules/**",
            "**/.git/**",
            "**/__pycache__/**",
            "**/.vscode/**",
            "**/venv/**",
            "**/.venv/**",
            "**/build/**",
            "** /dist/**",
            "**/out/**",
            "**/*.pyc",
            "**/*.egg-info/**",
            "**/*.o",
            "**/*.a",
            "**/*.lib",
            "**/*.obj",
            "**/*.so",
            "**/*.dll",
            "**/*.exe",
            "**/*.class",
            "**/*.jar",
            "**/*.war",
            "**/*.ear",
            "*.zip",
            "*.tar",
            "*.gz",
            "*.rar",
            "*.7z",
            "*.png",
            "*.jpg",
            "*.jpeg",
            "*.gif",
            "*.bmp",
            "*.svg",
            "*.ico",
            "*.lock"
          ],
          "description": "%config.contextFileExceptions.description%"
        },
        "lollmsVsCoder.language": {
          "type": "string",
          "default": "auto",
          "description": "%config.language.description%",
          "enum": [
            "auto",
            "en",
            "fr",
            "es",
            "de",
            "zh-cn",
            "ar"
          ]
        },
        "lollmsVsCoder.failsafeContextSize": {
          "type": "number",
          "default": 4096,
          "description": "Fallback context size to use if the API fails to return the context size."
        },
        "lollmsVsCoder.mcpServers": {
          "type": "object",
          "default": {},
          "description": "Configuration for MCP Servers. Map server names to their command execution strings.",
          "additionalProperties": {
            "type": "string"
          }
        },
        "lollmsVsCoder.companion.enableWebSearch": {
          "type": "boolean",
          "default": false,
          "description": "Allow the Companion (Quick Edit) to use Web Search."
        },
        "lollmsVsCoder.companion.enableArxivSearch": {
          "type": "boolean",
          "default": false,
          "description": "Allow the Companion (Quick Edit) to use ArXiv Search."
        },
        "lollmsVsCoder.userInfo.name": {
          "type": "string",
          "default": "",
          "description": "Your full name. Used by the AI for file headers and personalization."
        },
        "lollmsVsCoder.userInfo.email": {
          "type": "string",
          "default": "",
          "description": "Your email address. Used for file headers."
        },
        "lollmsVsCoder.userInfo.license": {
          "type": "string",
          "default": "MIT",
          "description": "Default license to use in generated code headers."
        },
        "lollmsVsCoder.userInfo.codingStyle": {
          "type": "string",
          "default": "",
          "description": "Brief description of your preferred coding style (e.g., 'Functional', 'PEP8', 'Concise')."
        },
        "lollmsVsCoder.herdParticipants": {
          "type": "array",
          "default": [],
          "description": "Deprecated: Use Pre-Answer and Post-Answer participants lists instead."
        },
        "lollmsVsCoder.herdPreAnswerParticipants": {
          "type": "array",
          "default": [],
          "description": "List of models and personalities for Pre-Answer Brainstorming (Phase 1)."
        },
        "lollmsVsCoder.herdPostAnswerParticipants": {
          "type": "array",
          "default": [],
          "description": "List of models and personalities for Post-Answer Brainstorming (Phase 3)."
        },
        "lollmsVsCoder.herdRounds": {
          "type": "number",
          "default": 2,
          "description": "Default number of rounds for Herd Mode brainstorming."
        },
        "lollmsVsCoder.herdDynamicMode": {
          "type": "boolean",
          "default": false,
          "description": "Enable Dynamic Herd Mode: The AI generates participants automatically based on the prompt."
        },
        "lollmsVsCoder.herdDynamicModelPool": {
          "type": "array",
          "default": [],
          "description": "Pool of available models for Dynamic Herd assignment. Each entry is { model: string, description: string }."
        },
        "lollmsVsCoder.git.deleteBranchAfterMerge": {
            "type": "boolean",
            "default": true,
            "description": "Automatically delete the temporary feature branch after merging it into the original branch."
        },
        "lollmsVsCoder.git.unstagedChangesBehavior": {
            "type": "string",
            "default": "stash",
            "enum": ["stash", "error", "keep"],
            "description": "Action to take if unstaged changes exist when creating a new AI branch.",
            "enumDescriptions": [
                "Stash changes and pop them after switching (if possible) or keep stashed.",
                "Abort the operation.",
                "Keep changes and carry them over to the new branch."
            ]
        },
        "lollmsVsCoder.noThinkMode": {
          "type": "boolean",
          "default": false,
          "description": "Prefixes all system prompts with the /no_think command to disable the AI's thinking process (for models that support it)."
        },
        "lollmsVsCoder.systemEnv.showOs": {
          "type": "boolean",
          "default": true,
          "description": "Automatically share your Operating System name with the AI in the system prompt."
        },
        "lollmsVsCoder.systemEnv.showIp": {
          "type": "boolean",
          "default": false,
          "description": "Automatically share your local IP addresses with the AI (Useful for network-related tasks)."
        },
        "lollmsVsCoder.systemEnv.showShells": {
          "type": "boolean",
          "default": true,
          "description": "Automatically detect and share available shells (powershell, bash, etc.) with the AI."
        },
        "lollmsVsCoder.systemEnv.customInfo": {
          "type": "string",
          "default": "",
          "description": "Provide additional environment context for the AI (e.g. 'I am using Git Bash on Windows', 'My Python version is 3.12')."
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lollms",
          "title": "%views.container.title%",
          "icon": "media/lollms-icon.svg"
        }
      ]
    },
    "views": {
      "lollms": [
        {
          "id": "lollmsActionsView",
          "name": "%view.actions.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsDiscussionsView",
          "name": "%view.discussions.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsProcessesView",
          "name": "%view.processes.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsPersonalitiesView",
          "name": "Personalities",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsCodeExplorerView",
          "name": "%view.codeExplorer.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsWorkflowsView",
          "name": "Workflows",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsSkillsView",
          "name": "%view.skills.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsChatPromptsView",
          "name": "%view.chatPrompts.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsCodeActionsView",
          "name": "%view.codeActions.name%",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "submenu": "lollms-vs-coder.setContextSubmenu",
          "group": "1_modification@0"
        },
        {
          "command": "lollms-vs-coder.inspectFile",
          "group": "1_modification@1",
          "when": "!resourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "lollms-vs-coder.learnSelectionAsSkill",
          "when": "editorHasSelection",
          "group": "1_modification"
        }
      ],
      "terminal/context": [
        {
          "submenu": "lollms-vs-coder.terminalSelectionSubmenu",
          "when": "terminalHasSelection",
          "group": "navigation"
        }
      ],
      "lollms-vs-coder.terminalSelectionSubmenu": [
        {
          "command": "lollms-vs-coder.sendSelectionToNewDiscussion",
          "group": "1_send"
        },
        {
          "command": "lollms-vs-coder.sendSelectionToLastDiscussion",
          "group": "1_send"
        }
      ],
      "lollms-vs-coder.setContextSubmenu": [
        {
          "command": "lollms-vs-coder.setContextIncluded",
          "group": "1_modification@1"
        },
        {
          "command": "lollms-vs-coder.setContextTreeOnly",
          "group": "1_modification@2"
        },
        {
          "command": "lollms-vs-coder.setContextDefinitionsOnly",
          "group": "1_modification@3"
        },
        {
          "command": "lollms-vs-coder.setContextExcluded",
          "group": "1_modification@4"
        },
        {
          "command": "lollms-vs-coder.setContextCollapsed",
          "group": "1_modification@5",
          "when": "resourceIsFolder"
        }
      ],
      "scm/title": [
        {
          "command": "lollms-vs-coder.generateCommitMessage",
          "when": "scmProvider == 'git'",
          "group": "navigation"
        },
        {
          "command": "lollms-vs-coder.inspectCommit",
          "when": "scmProvider == 'git'",
          "group": "navigation",
          "icon": "$(telescope)"
        },
        {
          "command": "lollms-vs-coder.gitManager",
          "when": "scmProvider == 'git'",
          "group": "navigation",
          "icon": "$(history)"
        }
      ],
      "debug/toolbar": [
        {
          "command": "lollms-vs-coder.debugErrorWithAI",
          "when": "debugState == 'stopped' && lollms:hasDebugError",
          "group": "navigation@7"
        },
        {
          "command": "lollms-vs-coder.debugErrorSendToDiscussion",
          "when": "debugState == 'stopped' && lollms:hasDebugError",
          "group": "navigation@8"
        }
      ],
      "editor/title": [
        {
          "command": "lollms-vs-coder.debugErrorWithAI",
          "when": "debugState == 'stopped' && lollms:hasDebugError",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.convertNotebookToScript",
          "when": "resourceExtname == .ipynb",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "lollms-vs-coder.newDiscussion",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.newTempDiscussion",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@2"
        },
        {
          "command": "lollms-vs-coder.newDiscussionFromClipboard",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@3"
        },
        {
          "command": "lollms-vs-coder.createDiscussionGroup",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@4"
        },
        {
          "command": "lollms-vs-coder.refreshDiscussions",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@5"
        },
        {
          "command": "lollms-vs-coder.titleAllDiscussions",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@6"
        },
        {
          "command": "lollms-vs-coder.cleanEmptyDiscussions",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@7"
        },
        {
          "command": "lollms-vs-coder.createPersonality",
          "when": "view == lollmsPersonalitiesView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.buildCodeGraph",
          "when": "view == lollmsCodeExplorerView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.addSkill",
          "when": "view == lollmsSkillsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.importSkills",
          "when": "view == lollmsSkillsView",
          "group": "navigation@2"
        },
        {
          "command": "lollms-vs-coder.exportSkills",
          "when": "view == lollmsSkillsView",
          "group": "navigation@3"
        },
        {
          "command": "lollms-vs-coder.createPrompt",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.createPromptGroup",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@2"
        },
        {
          "command": "lollms-vs-coder.refreshPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@3"
        },
        {
          "command": "lollms-vs-coder.importPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@4"
        },
        {
          "command": "lollms-vs-coder.exportPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "lollms-vs-coder.renameDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "inline@1"
        },
        {
          "command": "lollms-vs-coder.generateDiscussionTitle",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "inline@2"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "inline@3"
        },
        {
          "command": "lollms-vs-coder.renameDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.generateDiscussionTitle",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.moveDiscussionToGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "2_move"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.renameDiscussionGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussionGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.editPersonality",
          "when": "view == lollmsPersonalitiesView && viewItem == 'personality'",
          "group": "inline"
        },
        {
          "command": "lollms-vs-coder.editPersonality",
          "when": "view == lollmsPersonalitiesView && viewItem == 'personality'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.deletePersonality",
          "when": "view == lollmsPersonalitiesView && viewItem == 'personality'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.editPrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.movePromptToGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "2_move"
        },
        {
          "command": "lollms-vs-coder.deletePrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.renamePromptGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.deletePromptGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.deletePrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "9_delete"
        }
      ]
    },
    "notebookCellStatusBar": [
      {
        "viewType": "jupyter-notebook",
        "placement": "left",
        "priority": 100
      }
    ],
    "submenus": [
      {
        "id": "lollms-vs-coder.setContextSubmenu",
        "label": "%menu.lollmsAIContext.title%"
      },
      {
        "id": "lollms-vs-coder.terminalSelectionSubmenu",
        "label": "%menu.lollmsTerminalSelection.title%"
      }
    ]  
  },
  "scripts": {
    "vscode:prepublish": "npm run esbuild-package",
    "build-webview": "esbuild ./src/commands/chatPanel/webview/main.ts --bundle --outfile=out/webview/chatPanel.bundle.js --format=iife --platform=browser --target=es2020 --sourcemap && esbuild ./src/webview/codeGraph.ts --bundle --outfile=out/webview/codeGraph.bundle.js --format=iife --platform=browser --target=es2020 --sourcemap",
    "minify-webview": "esbuild ./src/commands/chatPanel/webview/main.ts --bundle --outfile=out/webview/chatPanel.bundle.js --format=iife --platform=browser --target=es2020 --minify && esbuild ./src/webview/codeGraph.ts --bundle --outfile=out/webview/codeGraph.bundle.js --format=iife --platform=browser --target=es2020 --minify",
    "compile": "npm run clean && npm run copy-assets && npm run build-webview && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --target=es2020 --sourcemap",
    "watch": "npm run clean && npm run copy-assets && npm run build-webview && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --target=es2020 --sourcemap --watch",
    "esbuild-package": "npm run clean && npm run copy-assets && npm run minify-webview && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --target=es2020 --minify",
    "copy-assets": "cpx \"src/commands/chatPanel/webview/*.css\" out/webview && cpx \"media/**\" out/media && cpx \"node_modules/@vscode/codicons/dist/codicon.{css,ttf}\" out/styles && cpx \"node_modules/prismjs/themes/prism-tomorrow.css\" out/styles",
    "package": "npm run build-and-package",
    "build-and-package": "npm run clean && npm run vscode:prepublish && npx @vscode/vsce package",
    "pretest": "npm run compile && npm audit",
    "test": "node ./out/test/runTest.js",
    "security-audit": "npm audit --audit-level moderate",
    "clean": "rimraf out"
  },
