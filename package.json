{
  "name": "lollms-vs-coder",
  "displayName": "Lollms VS Coder",
  "description": "AI-powered Visual Studio Code extension using Lollms OpenAI-compatible interface for smart coding, debugging, real-time enhancements, context management, and git commit message generation.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lollms-vs-coder.startChat",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lollms-vs-coder.startChat",
        "title": "Lollms: Start Chat"
      },
      {
        "command": "lollms-vs-coder.addFileToContext",
        "title": "Add File to Lollms AI Context"
      },
      {
        "command": "lollms-vs-coder.removeFileFromContext",
        "title": "Remove File from Lollms AI Context"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Lollms VS Coder Configuration",
      "properties": {
        "lollmsVsCoder.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for authenticating with the Lollms API."
        },
        "lollmsVsCoder.apiHost": {
          "type": "string",
          "default": "http://localhost:9642",
          "description": "URL of the Lollms API server (including protocol and port)."
        }
      }
    },  
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lollmsSettings",
          "title": "Lollms Settings",
          "icon": "media/lollms-icon.svg"
        }
      ]
    },
    "views": {
      "lollmsSettings": [
        {
          "id": "lollmsConfigView",
          "name": "Configuration"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0"
  },
  "dependencies": {}
}
