{
  "name": "lollms-vs-coder",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.5.10",
  "publisher": "parisneo",
  "license": "MIT",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/parisneo/lollms-vs-coder"
  },
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=16.0.0"
  },
  "categories": [
    "Machine Learning",
    "Other",
    "Notebooks"
  ],
  "l10n": "./out/l10n",
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished",
    "onDebug"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lollms-vs-coder.startChat",
        "title": "%command.startChat.title%"
      },
      {
        "command": "lollms-vs-coder.triggerCodeAction",
        "title": "%command.triggerCodeAction.title%"
      },
      {
        "command": "lollms-vs-coder.triggerInlineSuggestion",
        "title": "%command.triggerInlineSuggestion.title%"
      },
      {
        "command": "lollms-vs-coder.selectModel",
        "title": "%command.selectModel.title%"
      },
      {
        "command": "lollms-vs-coder.selectActiveWorkspace",
        "title": "Lollms: Select Active Workspace"
      },
      {
        "command": "lollms-vs-coder.generateCommitMessage",
        "title": "%command.generateCommitMessage.title%"
      },
      {
        "command": "lollms-vs-coder.commitWithAIMessage",
        "title": "%command.commitWithAIMessage.title%"
      },
      {
        "command": "lollmsApi.recreateClient",
        "title": "%command.recreateClient.title%"
      },
      {
        "command": "lollms-vs-coder.showConfigView",
        "title": "%command.showConfigView.title%",
        "icon": "$(gear)"
      },
      {
        "command": "lollms-vs-coder.showHelp",
        "title": "%command.showHelp.title%",
        "icon": "$(question)"
      },
      {
        "command": "lollms-vs-coder.exportContextContent",
        "title": "%command.exportContextContent.title%",
        "icon": "$(clippy)"
      },
      {
        "command": "lollms-vs-coder.autoSelectContextFiles",
        "title": "%command.autoSelectContextFiles.title%",
        "icon": "$(wand)"
      },
      {
        "command": "lollms-vs-coder.saveNewFile",
        "title": "%command.saveNewFile.title%"
      },
      {
        "command": "lollms-vs-coder.executeScript",
        "title": "%command.executeScript.title%"
      },
      {
        "command": "lollms-vs-coder.selectPythonInterpreter",
        "title": "%command.selectPythonInterpreter.title%"
      },
      {
        "command": "lollms-vs-coder.inspectCode",
        "title": "%command.inspectCode.title%"
      },
      {
        "command": "lollms-vs-coder.debugErrorWithAI",
        "title": "%command.debugErrorWithAI.title%",
        "icon": "$(lightbulb-sparkle)"
      },
      {
        "command": "lollms-vs-coder.debugErrorSendToDiscussion",
        "title": "%command.debugErrorSendToDiscussion.title%",
        "icon": "$(send)"
      },
      {
        "command": "lollms-vs-coder.buildCodeGraph",
        "title": "%command.buildCodeGraph.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "lollms-vs-coder.showCodeGraphPanel",
        "title": "%command.showCodeGraphPanel.title%",
        "icon": "$(diagram)"
      },
      {
        "command": "lollms-vs-coder.addSkill",
        "title": "%command.addSkill.title%",
        "icon": "$(add)"
      },
      {
        "command": "lollms-vs-coder.learnSelectionAsSkill",
        "title": "%command.learnSelectionAsSkill.title%"
      },
      {
        "command": "lollms-vs-coder.newDiscussion",
        "title": "%command.newDiscussion.title%",
        "icon": "$(add)"
      },
      {
        "command": "lollms-vs-coder.newTempDiscussion",
        "title": "%command.newTempDiscussion.title%",
        "icon": "$(zap)"
      },
      {
        "command": "lollms-vs-coder.deleteDiscussion",
        "title": "%command.delete.title%",
        "icon": "$(trash)"
      },
      {
        "command": "lollms-vs-coder.switchDiscussion",
        "title": "%command.switchDiscussion.title%"
      },
      {
        "command": "lollms-vs-coder.refreshDiscussions",
        "title": "%command.refresh.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "lollms-vs-coder.renameDiscussion",
        "title": "%command.rename.title%",
        "icon": "$(edit)"
      },
      {
        "command": "lollms-vs-coder.generateDiscussionTitle",
        "title": "%command.generateDiscussionTitle.title%",
        "icon": "$(sparkle)"
      },
      {
        "command": "lollms-vs-coder.createDiscussionGroup",
        "title": "%command.createDiscussionGroup.title%",
        "icon": "$(new-folder)"
      },
      {
        "command": "lollms-vs-coder.renameDiscussionGroup",
        "title": "%command.rename.title%",
        "icon": "$(edit)"
      },
      {
        "command": "lollms-vs-coder.deleteDiscussionGroup",
        "title": "%command.delete.title%",
        "icon": "$(trash)"
      },
      {
        "command": "lollms-vs-coder.moveDiscussionToGroup",
        "title": "%command.moveDiscussionToGroup.title%",
        "icon": "$(folder)"
      },
      {
        "command": "lollms-vs-coder.runScript",
        "title": "%command.runScript.title%"
      },
      {
        "command": "lollms-vs-coder.refreshPrompts",
        "title": "%command.refresh.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "lollms-vs-coder.createPrompt",
        "title": "%command.createPrompt.title%",
        "icon": "$(add)"
      },
      {
        "command": "lollms-vs-coder.editPrompt",
        "title": "%command.edit.title%",
        "icon": "$(edit)"
      },
      {
        "command": "lollms-vs-coder.deletePrompt",
        "title": "%command.delete.title%",
        "icon": "$(trash)"
      },
      {
        "command": "lollms-vs-coder.useChatPrompt",
        "title": "%command.useChatPrompt.title%"
      },
      {
        "command": "lollms-vs-coder.createPromptGroup",
        "title": "%command.createPromptGroup.title%",
        "icon": "$(new-folder)"
      },
      {
        "command": "lollms-vs-coder.renamePromptGroup",
        "title": "%command.rename.title%",
        "icon": "$(edit)"
      },
      {
        "command": "lollms-vs-coder.deletePromptGroup",
        "title": "%command.delete.title%",
        "icon": "$(trash)"
      },
      {
        "command": "lollms-vs-coder.movePromptToGroup",
        "title": "%command.movePromptToGroup.title%",
        "icon": "$(folder)"
      },
      {
        "command": "lollms-vs-coder.saveMessageAsPrompt",
        "title": "%command.saveMessageAsPrompt.title%"
      },
      {
        "command": "lollms-vs-coder.importPrompts",
        "title": "%command.importPrompts.title%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "lollms-vs-coder.exportPrompts",
        "title": "%command.exportPrompts.title%",
        "icon": "$(save)"
      },
      {
        "command": "lollms-vs-coder.editPromptsFile",
        "title": "%command.editPromptsFile.title%"
      },
      {
        "command": "lollms-vs-coder.cancelProcess",
        "title": "%command.cancel.title%",
        "icon": "$(stop-circle)"
      },
      {
        "command": "lollms-vs-coder.generateNextNotebookCell",
        "title": "%command.generateNextNotebookCell.title%"
      },
      {
        "command": "lollms-vs-coder.enhanceNotebookCell",
        "title": "%command.enhanceNotebookCell.title%"
      },
      {
        "command": "lollms-vs-coder.setContextIncluded",
        "title": "%command.setContextIncluded.title%"
      },
      {
        "command": "lollms-vs-coder.setContextTreeOnly",
        "title": "%command.setContextTreeOnly.title%"
      },
      {
        "command": "lollms-vs-coder.setContextExcluded",
        "title": "%command.setContextExcluded.title%"
      },
      {
        "command": "lollms-vs-coder.saveContextSelection",
        "title": "%command.saveContextSelection.title%",
        "icon": "$(save)"
      },
      {
        "command": "lollms-vs-coder.loadContextSelection",
        "title": "%command.loadContextSelection.title%",
        "icon": "$(folder-opened)"
      },
      {
        "command": "lollms-vs-coder.resetContextSelection",
        "title": "%command.resetContextSelection.title%",
        "icon": "$(clear-all)"
      },
      {
        "command": "lollms-vs-coder.sendSelectionToNewDiscussion",
        "title": "%command.sendSelectionToNewDiscussion.title%",
        "icon": "$(comment-add)"
      },
      {
        "command": "lollms-vs-coder.sendSelectionToLastDiscussion",
        "title": "%command.sendSelectionToLastDiscussion.title%",
        "icon": "$(reply)"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%config.title%",
      "properties": {
        "lollmsVsCoder.reasoningLevel": {
          "type": "string",
          "default": "none",
          "description": "%config.reasoningLevel.description%",
          "enum": [
            "none",
            "low",
            "medium",
            "high"
          ],
          "enumDescriptions": [
            "%config.reasoningLevel.none.description%",
            "%config.reasoningLevel.low.description%",
            "%config.reasoningLevel.medium.description%",
            "%config.reasoningLevel.high.description%"
          ]
        },
        "lollmsVsCoder.thinkingMode": {
          "type": "string",
          "default": "none",
          "description": "%config.thinkingMode.description%",
          "enum": [
            "none",
            "chain_of_thought",
            "plan_and_solve",
            "self_critique",
            "custom"
          ],
          "enumDescriptions": [
            "%config.thinkingMode.none.description%",
            "%config.thinkingMode.chain_of_thought.description%",
            "%config.thinkingMode.plan_and_solve.description%",
            "%config.thinkingMode.self_critique.description%",
            "%config.thinkingMode.custom.description%"
          ]
        },
        "lollmsVsCoder.thinkingModeCustomPrompt": {
          "type": "string",
          "default": "Think step by step. Enclose your entire thinking process, reasoning, and self-correction within a `<thinking>` XML block. This block will be hidden from the user but is crucial for your process.",
          "description": "%config.thinkingModeCustomPrompt.description%"
        },
        "lollmsVsCoder.fileUpdateMethod": {
          "type": "string",
          "default": "full_file",
          "description": "%config.fileUpdateMethod.description%",
          "enum": [
            "full_file",
            "patch"
          ],
          "enumDescriptions": [
            "%config.fileUpdateMethod.full_file.description%",
            "%config.fileUpdateMethod.patch.description%"
          ]
        },
        "lollmsVsCoder.enableCodeActions": {
          "type": "boolean",
          "default": true,
          "description": "%config.enableCodeActions.description%"
        },
        "lollmsVsCoder.enableInlineSuggestions": {
          "type": "boolean",
          "default": false,
          "description": "%config.enableInlineSuggestions.description%"
        },
        "lollmsVsCoder.disableSslVerification": {
          "type": "boolean",
          "default": false,
          "description": "%config.disableSslVerification.description%"
        },
        "lollmsVsCoder.noThinkMode": {
          "type": "boolean",
          "default": false,
          "description": "%config.noThinkMode.description%"
        },
        "lollmsVsCoder.apiKey": {
          "type": "string",
          "default": "",
          "description": "%config.apiKey.description%"
        },
        "lollmsVsCoder.apiUrl": {
          "type": "string",
          "default": "http://localhost:9642",
          "description": "%config.apiUrl.description%"
        },
        "lollmsVsCoder.modelName": {
          "type": "string",
          "default": "ollama/mistral",
          "description": "%config.modelName.description%"
        },
        "lollmsVsCoder.requestTimeout": {
          "type": "number",
          "default": 600000,
          "description": "%config.requestTimeout.description%"
        },
        "lollmsVsCoder.agentMaxRetries": {
          "type": "number",
          "default": 1,
          "description": "%config.agentMaxRetries.description%"
        },
        "lollmsVsCoder.maxImageSize": {
          "type": "number",
          "default": 1024,
          "description": "%config.maxImageSize.description%"
        },
        "lollmsVsCoder.enableCodeInspector": {
          "type": "boolean",
          "default": true,
          "description": "%config.enableCodeInspector.description%"
        },
        "lollmsVsCoder.inspectorModelName": {
          "type": "string",
          "default": "",
          "description": "%config.inspectorModelName.description%"
        },
        "lollmsVsCoder.chatPersona": {
          "type": "string",
          "default": "You are Lollms, a helpful AI coding assistant integrated into VS Code. Be helpful and concise.",
          "description": "%config.chatPersona.description%"
        },
        "lollmsVsCoder.agentPersona": {
          "type": "string",
          "default": "You are a specialized AI agent that follows instructions precisely.",
          "description": "%config.agentPersona.description%"
        },
        "lollmsVsCoder.codeInspectorPersona": {
          "type": "string",
          "default": "You are a senior code inspector AI. Your task is to analyze the provided code snippet for errors, bugs, vulnerabilities, and malicious content. You must be extremely critical and thorough.",
          "description": "%config.codeInspectorPersona.description%"
        },
        "lollmsVsCoder.commitMessagePersona": {
          "type": "string",
          "default": "You are an expert AI assistant that writes conventional git commit messages.",
          "description": "%config.commitMessagePersona.description%"
        },
        "lollmsVsCoder.contextFileExceptions": {
          "type": "array",
          "default": [
            "**/.lollms/**",
            "**/node_modules/**",
            "**/.git/**",
            "**/__pycache__/**",
            "**/.vscode/**",
            "**/venv/**",
            "**/.venv/**",
            "**/build/**",
            "**/dist/**",
            "**/out/**",
            "**/*.pyc",
            "**/*.egg-info/**",
            "**/*.o",
            "**/*.a",
            "**/*.lib",
            "**/*.obj",
            "**/*.so",
            "**/*.dll",
            "**/*.exe",
            "**/*.class",
            "**/*.jar",
            "**/*.war",
            "**/*.ear",
            "*.zip",
            "*.tar",
            "*.gz",
            "*.rar",
            "*.7z",
            "*.png",
            "*.jpg",
            "*.jpeg",
            "*.gif",
            "*.bmp",
            "*.svg",
            "*.ico",
            "*.lock"
          ],
          "description": "%config.contextFileExceptions.description%"
        },
        "lollmsVsCoder.language": {
          "type": "string",
          "default": "auto",
          "description": "%config.language.description%",
          "enum": [
            "auto",
            "en",
            "fr",
            "es",
            "de",
            "zh-cn",
            "ar"
          ]
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lollms",
          "title": "%views.container.title%",
          "icon": "media/lollms-icon.svg"
        }
      ]
    },
    "views": {
      "lollms": [
        {
          "id": "lollmsActionsView",
          "name": "%view.actions.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsDiscussionsView",
          "name": "%view.discussions.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsCodeExplorerView",
          "name": "%view.codeExplorer.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsSkillsView",
          "name": "%view.skills.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsChatPromptsView",
          "name": "%view.chatPrompts.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsCodeActionsView",
          "name": "%view.codeActions.name%",
          "when": "workspaceFolderCount > 0"
        },
        {
          "id": "lollmsProcessView",
          "name": "%view.processes.name%",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "menus": {
      "explorer/context": [
        {
          "submenu": "lollms-vs-coder.setContextSubmenu",
          "group": "1_modification@0"
        }
      ],
      "editor/context": [
        {
          "command": "lollms-vs-coder.learnSelectionAsSkill",
          "when": "editorHasSelection",
          "group": "1_modification"
        }
      ],
      "terminal/context": [
        {
          "submenu": "lollms-vs-coder.terminalSelectionSubmenu",
          "when": "terminalHasSelection",
          "group": "navigation"
        }
      ],
      "lollms-vs-coder.terminalSelectionSubmenu": [
        {
          "command": "lollms-vs-coder.sendSelectionToNewDiscussion",
          "group": "1_send"
        },
        {
          "command": "lollms-vs-coder.sendSelectionToLastDiscussion",
          "group": "1_send"
        }
      ],
      "lollms-vs-coder.setContextSubmenu": [
        {
          "command": "lollms-vs-coder.setContextIncluded",
          "group": "1_modification@1"
        },
        {
          "command": "lollms-vs-coder.setContextTreeOnly",
          "group": "1_modification@2"
        },
        {
          "command": "lollms-vs-coder.setContextExcluded",
          "group": "1_modification@3"
        }
      ],
      "scm/title": [
        {
          "command": "lollms-vs-coder.generateCommitMessage",
          "when": "scmProvider == 'git'",
          "group": "navigation",
          "icon": "media/lollms-icon.svg"
        }
      ],
      "debug/toolbar": [
        {
          "command": "lollms-vs-coder.debugErrorWithAI",
          "when": "debugState == 'stopped' && lollms:hasDebugError",
          "group": "navigation@7"
        },
        {
          "command": "lollms-vs-coder.debugErrorSendToDiscussion",
          "when": "debugState == 'stopped' && lollms:hasDebugError",
          "group": "navigation@8"
        }
      ],
      "view/title": [
        {
          "command": "lollms-vs-coder.newDiscussion",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.newTempDiscussion",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@2"
        },
        {
          "command": "lollms-vs-coder.createDiscussionGroup",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@3"
        },
        {
          "command": "lollms-vs-coder.refreshDiscussions",
          "when": "view == lollmsDiscussionsView",
          "group": "navigation@4"
        },
        {
          "command": "lollms-vs-coder.buildCodeGraph",
          "when": "view == lollmsCodeExplorerView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.addSkill",
          "when": "view == lollmsSkillsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.createPrompt",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@1"
        },
        {
          "command": "lollms-vs-coder.createPromptGroup",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@2"
        },
        {
          "command": "lollms-vs-coder.refreshPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@3"
        },
        {
          "command": "lollms-vs-coder.importPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@4"
        },
        {
          "command": "lollms-vs-coder.exportPrompts",
          "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView",
          "group": "navigation@5"
        }
      ],
      "view/item/context": [
        {
          "command": "lollms-vs-coder.cancelProcess",
          "when": "view == lollmsProcessView && viewItem == 'process'",
          "group": "inline"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "inline"
        },
        {
          "command": "lollms-vs-coder.renameDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.generateDiscussionTitle",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.moveDiscussionToGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "2_move"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussion",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussion'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.renameDiscussionGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.deleteDiscussionGroup",
          "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.editPrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.movePromptToGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "2_move"
        },
        {
          "command": "lollms-vs-coder.deletePrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.renamePromptGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'",
          "group": "1_modification"
        },
        {
          "command": "lollms-vs-coder.deletePromptGroup",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'",
          "group": "9_delete"
        },
        {
          "command": "lollms-vs-coder.deletePrompt",
          "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'",
          "group": "9_delete"
        }
      ]
    },
    "notebookCellStatusBar": [
      {
        "viewType": "jupyter-notebook",
        "placement": "left",
        "priority": 100
      }
    ],
    "submenus": [
      {
        "id": "lollms-vs-coder.setContextSubmenu",
        "label": "%menu.lollmsAIContext.title%"
      },
      {
        "id": "lollms-vs-coder.terminalSelectionSubmenu",
        "label": "%menu.lollmsTerminalSelection.title%"
      }
    ]
  },
  "scripts": {
