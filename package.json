{
  "name": "lollms-vs-coder",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.3.1",
  "publisher": "parisneo",
  "license": "MIT",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/parisneo/lollms-vs-coder"
  },
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=16.0.0"
  },
  "categories": [
    "Machine Learning",
    "Other"
  ],
  "l10n": "./l10n",
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
        { "command": "lollms-vs-coder.startChat", "title": "%command.startChat.title%" },
        { "command": "lollms-vs-coder.triggerCodeAction", "title": "%command.triggerCodeAction.title%" },
        { "command": "lollms-vs-coder.triggerInlineSuggestion", "title": "%command.triggerInlineSuggestion.title%" },
        { "command": "lollms-vs-coder.selectModel", "title": "%command.selectModel.title%" },
        { "command": "lollms-vs-coder.addFileToContext", "title": "%command.addFileToContext.title%" },
        { "command": "lollms-vs-coder.removeFileFromContext", "title": "%command.removeFileFromContext.title%" },
        { "command": "lollms-vs-coder.generateCommitMessage", "title": "%command.generateCommitMessage.title%" },
        { "command": "lollms-vs-coder.commitWithAIMessage", "title": "%command.commitWithAIMessage.title%" },
        { "command": "lollmsApi.recreateClient", "title": "%command.recreateClient.title%" },
        { "command": "lollms-vs-coder.refreshTree", "title": "%command.refresh.title%" },
        { "command": "lollms-vs-coder.cycleFileState", "title": "%command.cycleFileState.title%", "icon": "$(sync)" },
        { "command": "lollms-vs-coder.addFolderToContext", "title": "%command.addFolderToContext.title%" },
        { "command": "lollms-vs-coder.removeFolderFromContext", "title": "%command.removeFolderFromContext.title%" },
        { "command": "lollms-vs-coder.showConfigView", "title": "%command.showConfigView.title%", "icon": "$(gear)" },
        { "command": "lollms-vs-coder.showHelp", "title": "%command.showHelp.title%", "icon": "$(question)" },
        { "command": "lollms-vs-coder.exportContextContent", "title": "%command.exportContextContent.title%" },
        { "command": "lollms-vs-coder.saveNewFile", "title": "%command.saveNewFile.title%" },
        { "command": "lollms-vs-coder.executeScript", "title": "%command.executeScript.title%" },
        { "command": "lollms-vs-coder.newDiscussion", "title": "%command.newDiscussion.title%", "icon": "$(add)" },
        { "command": "lollms-vs-coder.deleteDiscussion", "title": "%command.delete.title%", "icon": "$(trash)" },
        { "command": "lollms-vs-coder.switchDiscussion", "title": "%command.switchDiscussion.title%" },
        { "command": "lollms-vs-coder.refreshDiscussions", "title": "%command.refresh.title%", "icon": "$(refresh)" },
        { "command": "lollms-vs-coder.renameDiscussion", "title": "%command.rename.title%", "icon": "$(edit)" },
        { "command": "lollms-vs-coder.generateDiscussionTitle", "title": "%command.generateDiscussionTitle.title%", "icon": "$(sparkle)" },
        { "command": "lollms-vs-coder.createDiscussionGroup", "title": "%command.createDiscussionGroup.title%", "icon": "$(new-folder)" },
        { "command": "lollms-vs-coder.renameDiscussionGroup", "title": "%command.rename.title%", "icon": "$(edit)" },
        { "command": "lollms-vs-coder.deleteDiscussionGroup", "title": "%command.delete.title%", "icon": "$(trash)" },
        { "command": "lollms-vs-coder.moveDiscussionToGroup", "title": "%command.moveDiscussionToGroup.title%", "icon": "$(folder)" },
        { "command": "lollms-vs-coder.runScript", "title": "%command.runScript.title%" },
        { "command": "lollms-vs-coder.refreshPrompts", "title": "%command.refresh.title%", "icon": "$(refresh)" },
        { "command": "lollms-vs-coder.createPrompt", "title": "%command.createPrompt.title%", "icon": "$(add)" },
        { "command": "lollms-vs-coder.editPrompt", "title": "%command.edit.title%", "icon": "$(edit)" },
        { "command": "lollms-vs-coder.deletePrompt", "title": "%command.delete.title%", "icon": "$(trash)" },
        { "command": "lollms-vs-coder.useChatPrompt", "title": "%command.useChatPrompt.title%" },
        { "command": "lollms-vs-coder.createPromptGroup", "title": "%command.createPromptGroup.title%", "icon": "$(new-folder)" },
        { "command": "lollms-vs-coder.renamePromptGroup", "title": "%command.rename.title%", "icon": "$(edit)" },
        { "command": "lollms-vs-coder.deletePromptGroup", "title": "%command.delete.title%", "icon": "$(trash)" },
        { "command": "lollms-vs-coder.movePromptToGroup", "title": "%command.movePromptToGroup.title%", "icon": "$(folder)" },
        { "command": "lollms-vs-coder.saveMessageAsPrompt", "title": "%command.saveMessageAsPrompt.title%" },
        { "command": "lollms-vs-coder.importPrompts", "title": "%command.importPrompts.title%", "icon": "$(folder-opened)" },
        { "command": "lollms-vs-coder.exportPrompts", "title": "%command.exportPrompts.title%", "icon": "$(save)" },
        { "command": "lollms-vs-coder.editPromptsFile", "title": "%command.editPromptsFile.title%" }
    ],
    "configuration": {
        "type": "object",
        "title": "%config.title%",
        "properties": {
          "lollmsVsCoder.enableCodeActions": { "type": "boolean", "default": true, "description": "%config.enableCodeActions.description%" },
          "lollmsVsCoder.enableInlineSuggestions": { "type": "boolean", "default": false, "description": "%config.enableInlineSuggestions.description%" },
          "lollmsVsCoder.disableSslVerification": { "type": "boolean", "default": false, "description": "%config.disableSslVerification.description%" },
          "lollmsVsCoder.apiKey": { "type": "string", "default": "", "description": "%config.apiKey.description%" },
          "lollmsVsCoder.apiUrl": { "type": "string", "default": "http://localhost:9642", "description": "%config.apiUrl.description%" },
          "lollmsVsCoder.modelName": { "type": "string", "default": "ollama/mistral", "description": "%config.modelName.description%" },
          "lollmsVsCoder.requestTimeout": { "type": "number", "default": 600000, "description": "%config.requestTimeout.description%" },
          "lollmsVsCoder.agentMaxRetries": { "type": "number", "default": 1, "description": "%config.agentMaxRetries.description%" },
          "lollmsVsCoder.globalSystemPrompt": {
            "type": "string",
            "default": "",
            "description": "%config.globalSystemPrompt.description%"
          },
          "lollmsVsCoder.language": {
            "type": "string",
            "default": "auto",
            "description": "%config.language.description%",
            "enum": ["auto", "en", "fr", "es", "de", "zh-cn", "ar"]
          }
        }
      },
      "viewsContainers": {
        "activitybar": [ { "id": "lollmsSettings", "title": "%views.container.title%", "icon": "media/lollms-icon.svg" } ]
      },
      "views": {
        "lollmsSettings": [
          { "id": "lollmsDiscussionsView", "name": "%view.discussions.name%", "when": "workspaceFolderCount > 0" },
          { "id": "lollmsChatPromptsView", "name": "%view.chatPrompts.name%", "when": "workspaceFolderCount > 0" },
          { "id": "lollmsCodeActionsView", "name": "%view.codeActions.name%", "when": "workspaceFolderCount > 0" },
          { "id": "lollmsSettings.fileTreeView", "name": "%view.fileTreeView.name%", "when": "workspaceFolderCount > 0" }
        ]
      },
      "menus": {
        "scm/title": [
          {
            "command": "lollms-vs-coder.generateCommitMessage",
            "when": "scmProvider == 'git'",
            "group": "navigation"
          }
        ],
        "view/title": [
          { "command": "lollms-vs-coder.newDiscussion", "when": "view == lollmsDiscussionsView", "group": "navigation@1" },
          { "command": "lollms-vs-coder.createDiscussionGroup", "when": "view == lollmsDiscussionsView", "group": "navigation@2" },
          { "command": "lollms-vs-coder.refreshDiscussions", "when": "view == lollmsDiscussionsView", "group": "navigation@3" },
          { "command": "lollms-vs-coder.createPrompt", "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView", "group": "navigation@1" },
          { "command": "lollms-vs-coder.createPromptGroup", "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView", "group": "navigation@2" },
          { "command": "lollms-vs-coder.refreshPrompts", "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView", "group": "navigation@3" },
          { "command": "lollms-vs-coder.importPrompts", "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView", "group": "navigation@4" },
          { "command": "lollms-vs-coder.exportPrompts", "when": "view == lollmsChatPromptsView || view == lollmsCodeActionsView", "group": "navigation@5" },
          { "command": "lollms-vs-coder.refreshTree", "when": "view == lollmsSettings.fileTreeView", "group": "navigation@1" },
          { "command": "lollms-vs-coder.exportContextContent", "when": "view == lollmsSettings.fileTreeView", "group": "navigation@2" },
          { "command": "lollms-vs-coder.showHelp", "when": "view == lollmsDiscussionsView || view == lollmsChatPromptsView || view == lollmsCodeActionsView || view == lollmsSettings.fileTreeView", "group": "navigation@98", "icon": "$(question)" },
          { "command": "lollms-vs-coder.showConfigView", "when": "view == lollmsDiscussionsView || view == lollmsChatPromptsView || view == lollmsCodeActionsView || view == lollmsSettings.fileTreeView", "group": "navigation@99", "icon": "$(gear)" }
        ],
        "view/item/context": [
            { "command": "lollms-vs-coder.deleteDiscussion", "when": "view == lollmsDiscussionsView && viewItem == 'discussion'", "group": "inline" },
            { "command": "lollms-vs-coder.cycleFileState", "when": "view == lollmsSettings.fileTreeView && (viewItem == 'file' || viewItem == 'image' || viewItem == 'folder')", "group": "inline" },
            { "command": "lollms-vs-coder.addFolderToContext", "when": "view == lollmsSettings.fileTreeView && viewItem == 'folder'", "group": "1_modification" },
            { "command": "lollms-vs-coder.removeFolderFromContext", "when": "view == lollmsSettings.fileTreeView && viewItem == 'folder'", "group": "1_modification" },
            { "command": "lollms-vs-coder.renameDiscussion", "when": "view == lollmsDiscussionsView && viewItem == 'discussion'", "group": "1_modification" },
            { "command": "lollms-vs-coder.generateDiscussionTitle", "when": "view == lollmsDiscussionsView && viewItem == 'discussion'", "group": "1_modification" },
            { "command": "lollms-vs-coder.moveDiscussionToGroup", "when": "view == lollmsDiscussionsView && viewItem == 'discussion'", "group": "2_move" },
            { "command": "lollms-vs-coder.deleteDiscussion", "when": "view == lollmsDiscussionsView && viewItem == 'discussion'", "group": "9_delete" },
            { "command": "lollms-vs-coder.renameDiscussionGroup", "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'", "group": "1_modification" },
            { "command": "lollms-vs-coder.deleteDiscussionGroup", "when": "view == lollmsDiscussionsView && viewItem == 'discussionGroup'", "group": "9_delete" },
            { "command": "lollms-vs-coder.editPrompt", "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'", "group": "1_modification" },
            { "command": "lollms-vs-coder.movePromptToGroup", "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'", "group": "2_move" },
            { "command": "lollms-vs-coder.deletePrompt", "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'prompt'", "group": "9_delete" },
            { "command": "lollms-vs-coder.renamePromptGroup", "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'", "group": "1_modification" },
            { "command": "lollms-vs-coder.deletePromptGroup", "when": "(view == lollmsChatPromptsView || view == lollmsCodeActionsView) && viewItem == 'promptGroup'", "group": "9_delete" }
        ]
      }
  },
  "scripts": { "vscode:prepublish": "npm run compile", "compile": "tsc -p ./", "watch": "tsc -watch -p ./", "package": "npx @vscode/vsce package", "pretest": "npm run compile && npm audit", "test": "node ./out/test/runTest.js", "security-audit": "npm audit --audit-level moderate", "clean": "rimraf out node_modules package-lock.json" },
  "devDependencies": { "@types/jimp": "^0.2.28", "@types/marked": "5.0.2", "@types/node-fetch": "2.6.13", "@types/pdf-parse": "^1.1.4", "@types/vscode": "1.74.0", "typescript": "5.9.2", "@vscode/vsce": "^2.26.0" },
  "dependencies": { "@vscode/codicons": "^0.0.35", "exceljs": "^4.4.0", "jimp": "^0.22.10", "mammoth": "^1.7.2", "marked": "16.2.1", "node-fetch": "2.7.0", "pdf-parse": "^1.1.1" },
  "overrides": { "eslint": { "chalk": ">=5.0.0", "debug": ">=4.3.5", "ansi-styles": ">=6.0.0" } }
}