    "watch": "npm run clean && npm run copy-assets && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --target=es2020 --sourcemap --watch",
    "esbuild-package": "npm run clean && npm run copy-assets && esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --target=es2020 --minify",
    "copy-assets": "cpx \"src/**/*.html\" out && cpx \"l10n/**\" out/l10n && cpx \"media/**\" out/media",
    "package": "npm run build-and-package",
    "build-and-package": "npm run clean && npm run vscode:prepublish && npx @vscode/vsce package",
